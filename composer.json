{
  "name": "Cameron-Xie/Slim-Application",
  "description": "Slim Application",
  "keywords": [
    "slim",
    "Docker",
    "Codeception",
    "php_cs",
    "starter"
  ],
  "require": {
    "php": ">=7.2",
    "hassankhan/config": "^1.0",
    "lstrojny/functional-php": "^1.7",
    "slim/slim": "^3.9",
    "vlucas/phpdotenv": "^2.4"
  },
  "require-dev": {
    "codeception/codeception": "^2.4",
    "codeception/mockery-module": "^0.2.3",
    "codeception/phpbuiltinserver": "^1.4",
    "mockery/mockery": "^1.0",
    "slevomat/coding-standard": "^4.5",
    "squizlabs/php_codesniffer": "^3.2"
  },
  "autoload": {
    "psr-4": {
      "Acme\\": "src/Acme"
    },
    "files": [
      "src/Acme/Functional/Helper.php",
      "src/Acme/Slim/Helper.php"
    ]
  },
  "config": {
    "platform": {
      "php": "7.2"
    },
    "sort-packages": true,
    "optimize-autoloader": true
  }
}
